    "docs": "docsify init ./docs"
  },
  "typings": "./index.d.ts",
  "main": "./dist/rambdax",
  "version": "7.3.0",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "7.12.13",
